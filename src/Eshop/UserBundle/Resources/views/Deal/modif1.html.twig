{% extends('@EshopUser/entrepriselayoutss.html.twig') %}

{% block c %}

    <div class="container">
        <div class="col-sm-6">

            <h1 align="center">Modifier deal</h1>

            <div class="container">
                {{ form_start(form,{'attr':{'novalidate':'novalidate'}}) }}
                <div class="form-group">{{ form_row(form.titre,{'attr':{'class':'form-control'}}) }}
                    {{ form_row(form.photo,{'attr':{'class':'form-control'}}) }}
                    {{ form_row(form.dateExpiration,{'attr':{'class':'form-control'}}) }}
                    {{ form_row(form.description,{'attr':{'class':'form-control'}}) }}
                </div>
                <div class="button">
                    {{ form_end(form) }}
                </div>
            </div>
        </div></div>
{% endblock %}
